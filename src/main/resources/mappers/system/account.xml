<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tgcity.example.demo1.dal.mappers.system.AccountMapper">

    <select id="getAccountList" resultType="com.tgcity.example.demo1.dal.entity.system.AccountEntity">
        select
            *
        from
            w_account
        where
            user_account = #{user_account}
    </select>
</mapper>
